<div id="mooring-listings" class="container">


<% @moorings.each do |mooring| %>


  <div class="card">
    <%= cl_image_tag mooring.photo, height: 150, width: 200, crop: :fill %>

    <div class="card-body">
      <h5 class="card-title"><%= mooring.location %></h5>
      <p class="card-text">Price: <%= mooring.price %></p>
    </div>
    <%= link_to "show details", mooring_path(mooring)  %>
  </div>
<% end %>


</div>
